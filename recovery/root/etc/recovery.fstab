# Mount point		FStype					Device													Flags

/system       ext4        system     flags=display="System";backup=1;canbewiped;wipeingui
/cache        ext4        /dev/block/by-name/cache     flags=display="Cache";canbewiped;wipeingui;wipeduringfactoryreset
/data         f2fs        /dev/block/by-name/userdata     flags=length=-16384;display="Data";backup=1;canbewiped;wipeingui;wipeduringfactoryreset;settingsstorage;encryptable=/dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/metadata

/boot         emmc        /dev/block/by-name/boot      flags=display="Boot";backup=1;flashimg=1

# I couldn't find anything like these, so I'm gonna leave these as it is
# /logo         emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/logo          /dev/block/mmcblk0p9      flags=display="Logo";backup=1;flashimg=1
# /nvram        emmc        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/nvram         /dev/block/mmcblk0p2      flags=display="NvRAM";backup=1;flashimg=1
# /nvdata       ext4        /dev/block/platform/mtk-msdc.0/11230000.msdc0/by-name/nvdata        /dev/block/mmcblk0p18     flags=display="NvData";backup=1;flashimg=1

/recovery     emmc        /dev/block/by-name/recovery      flags=display="Recovery";backup=1;flashimg=1
/metadata     emmc        /dev/block/by-name/metadata

/external_sd  auto        /dev/block/mmcblk0p1                     flags=display="Micro SDcard";storage;wipeingui;removable;settingsstorage;backup=0;encryptable=userdata

# I couldn't find anything like these, so I'm gonna leave these as it is
/usbotg       auto        /dev/block/sda1                /dev/block/sda                         flags=display="USB-OTG";backup=0;wipeingui;storage;removable
# auto          vfat        /devices/mtk-msdc.0/11230000.msdc0*                   defaults        voldmanaged=sdcard0:auto
# auto          auto        /devices/mtk-msdc.0/11240000.msdc1*                   defaults        voldmanaged=sdcard1:auto;noemulatedsd;encryptable=userdata
# auto          vfat        /devices/platform/mt_usb*                             defaults        voldmanaged=usbotg:auto

